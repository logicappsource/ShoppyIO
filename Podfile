# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'


target 'Nike-Retail' do
    use_frameworks!

    # Firebase
    pod 'Firebase'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'

    # Stripe
    pod 'Stripe'

    # Networking
    pod 'AFNetworking'

end
